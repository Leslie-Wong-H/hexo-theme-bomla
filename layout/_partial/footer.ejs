<footer id="footer">
  <!-- <style>
		.footer-right>a:hover{
			color: #757575
		}
	</style> -->
  <div class="outer">
    <div id="footer-info">
      <div class="footer-left">
        &copy; <%= date(new Date(), 'YYYY') %> <%= config.author || config.title
        %>
      </div>
      <div class="footer-left">
        <a
          href="http://beian.miit.gov.cn/"
          target="_blank"
          style="color:#555;padding:0 7vw;"
        >
          <% if(theme.license['ICP']){ %> <%= theme.license['ICP'] %> <% } %>
        </a>
      </div>
      <div class="footer-left">
        <a
          href="http://www.beian.gov.cn/portal/index.do"
          target="_blank"
          style="color:#555;padding:0 7vw;"
        >
          <% if(theme.license['POLICE']){ %> <%= theme.license['POLICE'] %> <% }
          %>
        </a>
      </div>
      <div class="footer-right">
        <a href="http://hexo.io/" target="_blank" style="color: #666">Hexo</a>
        Theme
        <a
          href="https://github.com/Leslie-Wong-H/hexo-theme-bomla"
          target="_blank"
          style="color:#666"
          >Bomla</a
        >
        inspired from
        <a
          href="https://github.com/litten/hexo-theme-yilia"
          target="_blank"
          style="color: #666"
          >Yilia</a
        >
      </div>
    </div>
  </div>
</footer>
